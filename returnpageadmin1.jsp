    <%
   
            String rfid= request.getParameter("rfid");
            String phone= request.getParameter("phone");
            String sadd= request.getParameter("sadd");
            String dadd= request.getParameter("dadd");
            String name=request.getParameter("name");
            String tor= request.getParameter("tor");
            String dor= request.getParameter("dor");
            String massege= request.getParameter("massege");
            long refid;
            
            Random rd= new Random();
            
            refid =10000+ rd.nextInt(99999);
            
            try
            {
                Class.forName("org.apache.derby.jdbc.ClientDriver");
                Connection cn= DriverManager.getConnection("jdbc:derby://localhost:1527/DP");
                
                PreparedStatement ps= cn.prepareStatement("insert into return values(?,?,?,?,?,?,?,?,?)");
                ps.setString(1,refid+"");
                ps.setString(2,rfid);
                ps.setString(3,sadd);
                ps.setString(4,dadd);
                ps.setString(5,tor);
                ps.setString(6,dor);
                ps.setString(7,massege);
                ps.setString(8,name);
                ps.setString(9,phone);
                
                int c=ps.executeUpdate();
                out.print(c+"recored inserted");
                cn.close();
            }
            catch(Exception ex)
            {
                out.print(ex);
            }
            
        
    
